<?xml version="1.0" encoding="utf-8"?>
<errors xmlns="urn:platinum/actor" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="urn:platinum/actor ../../Platinum.Schema/PlatinumResxError.xsd">
    <actor base="{Application}.Algorithm" />

    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    ~
    ~ Luhn/
    ~ Errors thrown by Luhn class.
    ~
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <error id="Luhn_DuplicateCharacterInSet" actor="~#Runtime" code="7001">
        <description>Character '{1}' is declared twice in character set '{0}'.</description>
        <arg name="CharacterSet" />
        <arg name="Character" />
    </error>

    <error id="Luhn_CodeCharacterNotInSet" actor="~#Development" code="7002">
        <description>Character '{1}' is not valid for character set '{0}'.</description>
        <arg name="CharacterSet" />
        <arg name="Character" />
    </error>


    <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    ~
    ~ NumericEncoder/
    ~ Errors thrown by NumericEncoder class.
    ~
    ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
    <error id="NumericEncoder_DuplicateCharacterInSet" actor="~#Runtime" code="7001">
        <description>Character '{1}' is declared twice in character set '{0}'.</description>
        <arg name="CharacterSet" />
        <arg name="Character" />
    </error>

    <error id="NumericEncoder_CodeCharacterNotInSet" actor="~#Runtime" code="7003">
        <description>Character '{1}' is not valid for character set '{0}'.</description>
        <arg name="CharacterSet" />
        <arg name="Character" />
    </error>

</errors>